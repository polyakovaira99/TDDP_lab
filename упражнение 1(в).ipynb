   "source": [
    "s = input()\n",
    " \n",
    "i = 0\n",
    "while s[i] == ' ': i+=1\n",
    "s = s[i:]\n",
    " \n",
    "i = len(s)\n",
    "while s[i-1] == ' ': i-=1\n",
    "s = s[:i]\n",
    " \n",
    "s1 = s[0]\n",
    "i = 1\n",
    "while i < len(s):\n",
    " if s[i] != ' ':\n",
    "  s1 += s[i]\n",
    " elif s[i-1] != ' ':\n",
    "  s1 += '*'\n",
    " i += 1\n",
    "print(s1)"
